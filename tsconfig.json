{
    "compileOnSave": false,
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": true,
        "outDir": "tmp",
        "target": "ES5",
        "sourceMap": true,
        "moduleResolution": "node",
        "strictNullChecks": true,
        "types": [
            "jasmine"
        ],
        "strictFunctionTypes": true,
        "strictPropertyInitialization": true,
    },
    "exclude": [
        "node_modules"
    ]
}